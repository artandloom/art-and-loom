{
    "builds": [
        {
            "src": "nuxt.config.js",
            "use": "@nuxtjs/vercel-builder",
            "config": {}
        }
    ],
    "routes": [
        {
            "src": "/_nuxt/.+",
            "headers": {
                "Cache-Control": "max-age=31557600"
            }
        },
        {
            "src": "^/(.*)",
            "dest": "/"
        }
    ]
}